    "license": "MIT",
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        },
        {
            "name": "Kirill Nesmeyanov (SerafimArts)",
            "email": "kirill.nesmeyanov@spiralscout.com"
        },
        {
            "name": "Pavel Butchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
